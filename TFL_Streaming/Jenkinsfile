pipeline {
agent any

stages {
    stage('Run Spark Streaming Job') {
        steps {
            sh """
                spark-submit \
                  --master local \
                  --packages org.apache.spark:spark-sql-kafka-0-10_2.11:2.4.7,com.lihaoyi:requests_2.11:0.7.1 \
                  TFL_Streaming/src/stream_archive.py
            """
        }
    }
}
}